<?xml version="1.0"?>
<filecomparator>
	<name>sample</name>
	<description>sample configuration file</description>
	<source-dir>./test/original</source-dir>
	<target-dir>./test/revised</target-dir>
	<output-file></output-file>
	
	<filetypes>
		<filetype>
			<type>default</type>
			<plugins>
			</plugins>
		</filetype>
		<filetype>
			<type>java</type>
			<plugins>
				<plugin>
					<name>IgnoreRegularExpressionPlugin</name>
					<params>
						<param>
							<name>regexp</name>
							<value>^@Generated\(.*</value>
						</param>
					</params>
				</plugin>
				<plugin>
					<name>IgnoreMultilineCommentsPlugin</name>
				</plugin>
				<plugin>
					<name>IgnoreSingleLineCommentsPlugin</name>
				</plugin>
				<plugin>
					<name>IgnoreBlankPlugin</name>
				</plugin>
			</plugins>
		</filetype>
		<filetype>
			<type>xml</type>
			<plugins>
				<plugin>
					<name>IgnoreXmlCommentsPlugin</name>
				</plugin>
				<plugin>
					<name>IgnoreBlankPlugin</name>
				</plugin>
			</plugins>
		</filetype>
	</filetypes>
	
	<ignore-files>
		<ignore>class</ignore>
		<ignore>docx</ignore>
		<ignore>doc</ignore>
	</ignore-files>
	
</filecomparator>